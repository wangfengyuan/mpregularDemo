



















.main-wrap {
  box-sizing: border-box;
  width: 60%;
  margin: 0 auto;
  background: #fff;
  overflow: hidden;
}




























.data-lists {
  display: flex;
  padding: 30rpx;
  padding-left: 0;
  border-bottom: 1rpx solid #f5f5f5;
  color: rgb(78, 79, 84);
}

.first{
  display: block;
  box-sizing: border-box;
  height: 170rpx;
  overflow: hidden;
  padding: 15rpx 0;
  background-color: rgba(8,105,132,.9); 
  color: #60bed8;
  text-align: center; 
  border: none;
} 
.item-id{
  width: 80rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: bold;
  font-size: 14px;
  flex-grow: 0;
  flex-shrink: 0;
  text-align: center;
}
.first .item-id {
  display: none;
} 
.first image {
  border-radius: 50%;
}
.data-lists.first .item-right {
  padding: 0 20rpx;
  line-height: 20rpx;
}
.data-lists .item-right {
  flex-grow: 1;
  flex-shrink: 1;
  overflow: hidden;
  padding-left: 15rpx;
  font-size: 8px;
}
.data-lists .item-right .item-title {
  line-height: 30rpx;
  font-weight: bold;
}
.data-lists .item-right .item-desc {
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break:keep-all;
  word-wrap:normal;
}
.data-lists .item-img {
  width: 60rpx;
  height: 60rpx;
  flex-grow: 0;
  flex-shrink: 0
} 
@font-face {
    font-family: 'icomoon';
    src:  url(data:application/vnd.ms-fontobject;base64,);
    src:  url(data:application/vnd.ms-fontobject;base64,#iefix) format('embedded-opentype'),
      url(data:font/ttf;base64,) format('truetype'),
      url(data:application/font-woff;base64,) format('woff'),
      url(data:image/svg+xml;base64,#icomoon) format('svg');
    font-weight: normal;
    font-style: normal;
  }
  .icon {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon' !important;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
  
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  } 
  .icon-stats-dots:before {
    content: "\E99B";
  }
  .icon-fire:before {
    content: "\E9A9";
  }
  
























.header-wrap {
  width: 100%;
   height: 400rpx; 
  padding-top: 50rpx;
  background-image: linear-gradient(135deg,#79f1a4,#0e5cad);
  text-align: center;
  color: #fff
}
.header-wrap .header-title {
  font-size: 28px;
  font-weight: bold;
}
.header-wrap .tab-btn {
  width: 300rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 40rpx auto 0;
}
.header-wrap .tab-btn .btn-item{
  width: 85rpx;
  height: 30rpx;
  margin: 0 18rpx;
  padding: 10rpx;
  line-height: 30rpx;
  font-size: 8px;
  border: 1rpx solid #fff;
  cursor: pointer;
} 
.header-wrap .tab-btn .choose {
  background: #fff;
  color: #0099c3;
}
.header-wrap .tab-btn .btn-item .icon{
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
}
































page {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #f7f8fa;
  padding-bottom: 50rpx;
}
page .top-title {
  box-sizing: border-box;
  margin-bottom: -170rpx;
}
